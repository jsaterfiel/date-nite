  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.1.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "jquery": "^3.3.1",
    "moment": "^2.22.1",
    "mongodb": "^3.0.7",
    "mongodb-bluebird": "^0.1.2",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "popper.js": "^1.14.3",
    "proptypes": "^1.1.0",
    "react": "^16.2.0",
    "react-datepicker": "^1.4.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^1.1.1",
    "redis": "^2.8.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "start-client": "react-scripts start",
    "build-client": "react-scripts build",
    "eject-client": "react-scripts eject",
    "start-api": "node api/index.js",
    "watch-api": "nodemon --watch api api/index.js",
    "start-scheduler": "node scheduler/index.js",
    "watch-scheduler": "nodemon --watch scheduler scheduler/index.js",
    "start-redis": "redis-server /etc/redis.conf",
    "extractor": "node extractor/index.js",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "npm start-redis & npm run start-worker & npm run start-server & run-p watch-css start-client"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "babel-preset-airbnb": "^2.4.0",
    "standard": "^11.0.1"
  },
  "proxy": "http://localhost:3000/"
